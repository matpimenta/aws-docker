{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "extraordinaire/docker-gocd-server",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "8153"
    }
  ],
  "Volumes": [
    {
        "HostDirectory": "/var/app/gocd",
        "ContainerDirectory": "/etc/gocd"
    }
  ],
  "Logging": "/var/log/gocd"
}
